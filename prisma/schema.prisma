generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id                     Int    @id @default(autoincrement())
  firstName              String
  lastName               String
  age                    Int
  number_of_years_in_atl Int
  phonenumber            Int
  bus_number             String
  email                  String
  class                  String
  section                String
  user_type              String
}
